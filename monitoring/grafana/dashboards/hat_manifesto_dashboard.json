          {
            "expr": "rate(hat_manifesto_risk_events_total[5m])",
            "legendFormat": "Risk Events/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 10,
        "title": "Funding Rate Arbitrage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "hat_manifesto_funding_rate",
            "legendFormat": "Funding Rate"
          },
          {
