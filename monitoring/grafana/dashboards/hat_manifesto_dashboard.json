        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Slippage Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(hat_manifesto_slippage_bps_bucket[5m]))",
            "legendFormat": "P50 Slippage"
          },
          {
            "expr": "histogram_quantile(0.95, rate(hat_manifesto_slippage_bps_bucket[5m]))",
            "legendFormat": "P95 Slippage"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
