{
  "dashboard": {
    "id": null,
    "title": "Institutional Trading System - Real-Time Observability",
    "tags": ["trading", "institutional", "observability"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Score",
        "type": "singlestat",
        "span": 3,
        "targets": [
          {
            "expr": "system_health_score",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "colorBackground": true,
        "thresholds": "70,90",
        "colors": ["#d44a3a", "#e24d42", "#299c46"],
        "sparkline": {
          "show": true,
          "full": false,
          "lineColor": "rgb(31, 120, 193)",
          "fillColor": "rgba(31, 118, 189, 0.18)"
        },
        "gauge": {
          "show": true,
          "min": 0,
          "max": 100,
          "thresholdMarkers": true
        }
      },
      {
        "id": 2,
        "title": "Realized P&L (USD)",
        "type": "singlestat",
        "span": 3,
        "targets": [
          {
            "expr": "trading_pnl_realized_usd",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "currencyUSD",
        "colorValue": true,
        "thresholds": "0,1000",
        "colors": ["#d44a3a", "#e24d42", "#299c46"],
        "sparkline": {
          "show": true,
          "full": false
        }
      },
      {
        "id": 3,
        "title": "Current Drawdown",
        "type": "singlestat",
        "span": 3,
        "targets": [
          {
            "expr": "risk_drawdown_current_pct",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "colorBackground": true,
        "thresholds": "3,5",
        "colors": ["#299c46", "#e24d42", "#d44a3a"],
        "sparkline": {
          "show": true,
          "full": false
        }
      },
      {
        "id": 4,
        "title": "Open Positions",
        "type": "singlestat",
        "span": 3,
        "targets": [
          {
            "expr": "trading_positions_count",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "colorValue": true,
        "thresholds": "5,10",
        "colors": ["#299c46", "#e24d42", "#d44a3a"]
      },
      {
        "id": 5,
        "title": "Trading Performance",
        "type": "graph",
        "span": 6,
        "targets": [
          {
            "expr": "trading_pnl_realized_usd",
            "refId": "A",
            "legendFormat": "Realized P&L"
          },
          {
            "expr": "trading_pnl_unrealized_usd",
            "refId": "B",
            "legendFormat": "Unrealized P&L"
          },
          {
            "expr": "business_daily_pnl_usd",
            "refId": "C",
            "legendFormat": "Daily P&L"
          }
        ],
        "yAxes": [
          {
            "label": "USD",
            "min": null,
            "max": null
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true
        },
        "nullPointMode": "null",
        "steppedLine": false,
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        }
      },
      {
        "id": 6,
        "title": "Risk Metrics",
        "type": "graph",
        "span": 6,
        "targets": [
          {
            "expr": "risk_drawdown_current_pct",
            "refId": "A",
            "legendFormat": "Current Drawdown %"
          },
          {
            "expr": "performance_sharpe_ratio",
            "refId": "B",
            "legendFormat": "Sharpe Ratio"
          },
          {
            "expr": "business_win_rate_pct",
            "refId": "C",
            "legendFormat": "Win Rate %"
          }
        ],
        "yAxes": [
          {
            "label": "Ratio/Percent",
            "min": 0,
            "max": null
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true
        }
      },
      {
        "id": 7,
        "title": "Order Execution Latency",
        "type": "heatmap",
        "span": 6,
        "targets": [
          {
            "expr": "rate(system_latency_order_execution_seconds_bucket[5m])",
            "refId": "A",
            "legendFormat": "{{le}}"
          }
        ],
        "heatmap": {
          "xBucketSize": null,
          "xBucketNumber": null,
          "yBucketSize": null,
          "yBucketNumber": null
        },
        "cards": {
          "cardPadding": 2,
          "cardRound": 0
        },
        "color": {
          "mode": "spectrum",
          "cardColor": "#b4ff00",
          "colorScale": "sqrt",
          "exponent": 0.5,
          "colorScheme": "interpolateOranges"
        }
      },
      {
        "id": 8,
        "title": "Trade Execution Rate",
        "type": "graph",
        "span": 6,
        "targets": [
          {
            "expr": "rate(trading_trades_total[5m])",
            "refId": "A",
            "legendFormat": "Trades/sec"
          },
          {
            "expr": "rate(trading_trades_total{outcome=\"success\"}[5m])",
            "refId": "B",
            "legendFormat": "Successful Trades/sec"
          },
          {
            "expr": "rate(trading_trades_total{outcome=\"failed\"}[5m])",
            "refId": "C",
            "legendFormat": "Failed Trades/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Trades per second",
            "min": 0,
            "max": null
          }
        ]
      },
      {
        "id": 9,
        "title": "Position Values by Symbol",
        "type": "piechart",
        "span": 4,
        "targets": [
          {
            "expr": "trading_position_value_usd",
            "refId": "A",
            "legendFormat": "{{symbol}}"
          }
        ],
        "pieType": "donut",
        "legend": {
          "show": true,
          "values": true,
          "percent": true
        }
      },
      {
        "id": 10,
        "title": "System Resource Usage",
        "type": "graph",
        "span": 4,
        "targets": [
          {
            "expr": "system_cpu_percent",
            "refId": "A",
            "legendFormat": "CPU %"
          },
          {
            "expr": "system_memory_percent",
            "refId": "B",
            "legendFormat": "Memory %"
          }
        ],
        "yAxes": [
          {
            "label": "Percent",
            "min": 0,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 11,
        "title": "Active Alerts",
        "type": "table",
        "span": 4,
        "targets": [
          {
            "expr": "system_active_alerts",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "columns": [
          {
            "text": "Alert Name",
            "value": "alertname"
          },
          {
            "text": "Severity",
            "value": "severity"
          },
          {
            "text": "Value",
            "value": "value"
          },
          {
            "text": "Threshold",
            "value": "threshold"
          }
        ],
        "sort": {
          "col": 1,
          "desc": true
        }
      },
      {
        "id": 12,
        "title": "API Latency by Endpoint",
        "type": "graph",
        "span": 6,
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(system_latency_api_calls_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "P95 - {{endpoint}}"
          },
          {
            "expr": "histogram_quantile(0.99, rate(system_latency_api_calls_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "P99 - {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0,
            "max": null
          }
        ],
        "thresholds": [
          {
            "value": 0.2,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 13,
        "title": "Business Metrics",
        "type": "graph",
        "span": 6,
        "targets": [
          {
            "expr": "business_profit_factor",
            "refId": "A",
            "legendFormat": "Profit Factor"
          },
          {
            "expr": "business_win_rate_pct",
            "refId": "B",
            "legendFormat": "Win Rate %"
          }
        ],
        "yAxes": [
          {
            "label": "Ratio/Percent",
            "min": 0,
            "max": null
          }
        ],
        "thresholds": [
          {
            "value": 1.0,
            "colorMode": "critical",
            "op": "lt"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["1s", "5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "name": "Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "titleFormat": "{{alertname}}",
          "textFormat": "{{instance}}: {{summary}}"
        }
      ]
    },
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 1,
    "links": []
  }
}
